<div class="navigation">
  <header>
    <a href="<%- url_for() %>" class="mylogo"><%= config.title %></a>
  </header>
  <nav>
    <% for (var i in theme.menu){ %>
      <a href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
    <% } %>
  </nav>
  <footer>
    <% if (config.subtitle){ %>
    <h2><%= config.subtitle %></h2>
    <% } %>
  </footer>
</div>
<!-- /.navigation -->
