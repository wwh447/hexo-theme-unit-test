<div class="articles">
  <% page.posts.each(function(post){ %>
    <%- partial('article', {post: post, index: true}) %>
  <% }) %>
</div>
<!-- /.articles -->

<% if (page.total > 1){ %>
  <nav class="paging">
    <%- paginator({
      prev_text: "&laquo;",
      next_text: "&raquo;"
    }) %>
  </nav>
<% } %>
